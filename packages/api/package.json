    "@nestjs/websockets": "^8.2.6",
    "@prisma/client": "^3.8.1",
    "apollo-server-fastify": "^3.6.2",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "bull-mq-transport": "^3.0.1",
    "bullmq": "^1.68.1",
