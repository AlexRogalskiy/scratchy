    "@prisma/client": "^3.6.0",
    "apollo-server-fastify": "^3.5.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "bull-mq-transport": "^3.0.0",
    "bullmq": "^1.59.2",
    "class-transformer": "^0.5.1",
