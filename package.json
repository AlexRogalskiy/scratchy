  "scripts": {
    "typeorm": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "prebuild": "rimraf dist",
    "build": "nest build --config tsconfig.build.json",
    "format": "prettier --write \"{src,test}/**/*.(t|j)s*\"",
    "sync": "npm run typeorm schema:log",
    "start": "ts-node src/main.ts",
    "start:queue:dev": "ts-node-dev src/worker.ts",
    "start:dev": "ts-node-dev src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert",
    "migrate:create": "db-migrate create --sql-file -m db/migrations",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
