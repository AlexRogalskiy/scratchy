    "jwt-decode": "^2.2.0",
    "mjml": "^4.6.3",
    "node-fetch": "^3.0.0-beta.8",
    "pg": "^8.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2",
    "source-map-support": "^0.5.19",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^1.0.0",
    "typegraphql-nestjs": "^0.2.3",
