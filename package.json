  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.build.json",
    "format": "prettier --write \"{src,test}/**/*.(t|j)s*\"",
    "start": "ts-node src/main.ts",
    "start:dev": "ts-node-dev src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "migrate:up": "db-migrate up -m db/migrations",
    "migrate:down": "db-migrate down -m db/migrations",
    "migrate:down:all": "db-migrate down -m db/migrations 0",
    "migrate:create": "db-migrate create --sql-file -m db/migrations",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [],
